{
    "extends": "@tsconfig/svelte/tsconfig.json",
    "include": ["./ui", "./src"],
    "exclude": ["./node_modules/*", ".old/*", "./old/*"],
    "compilerOptions": {
        "strict": true,
        "baseUrl": ".",
        "paths": {
            "src/*": ["./src/*"],
            "trace/*": ["./src/tracetime/*"],
            "websocket/*": ["./src/websocket/*"],
            "ui/*": ["./ui/*"]
        }
    },
    "types": ["node", "svelte"]
}
