{
    "extends": "@tsconfig/svelte/tsconfig.json",
    "include": ["./src/ui", "./src/trace"],
    "exclude": ["./node_modules/*", ".old/*"],
    "compilerOptions": {
        "strict": true,
        "baseUrl": ".",
        "paths": {
            "src/*": ["./src/*"],
            "trace/*": ["./src/trace/*"],
            "websocket/*": ["./src/websocket/*"],
            "ui/*": ["./src/ui/*"]
        }
    }
}
