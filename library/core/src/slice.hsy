use crate::*

pub extern Slice<covariant E>;

impl<E> Slice E:
    pub fn len() -> usize;

    pub fn swap(&mut self, a: usize, b: usize);

pub extern CyclicSliceLeashed<covariant E>;

impl<E> core::ops::IntIndex for CyclicSliceLeashed E:
    type Output = E

impl<E> CyclicSliceLeashed E:
    pub fn ilen() -> i32;

    pub fn start() -> i32;
    
    pub fn end() -> i32;
    
    pub fn first() -> ?~E;

    pub fn last() -> ?~E;