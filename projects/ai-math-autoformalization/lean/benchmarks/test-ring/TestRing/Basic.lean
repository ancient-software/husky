namespace MyTheormes1

-- Basic properties of addition and multiplication
theorem add_assoc (a b c : ℤ) : a + b + c = a + (b + c) :=
by ring

theorem mul_assoc (a b c : ℤ) : a * b * c = a * (b * c) :=
by ring

theorem mul_add (a b c : ℤ) : a * (b + c) = a * b + a * c :=
by ring

theorem add_mul (a b c : ℤ) : (a + b) * c = a * c + b * c :=
by ring

theorem add_comm (a b : ℤ) : a + b = b + a :=
by ring

theorem mul_comm (a b : ℤ) : a * b = b * a :=
by ring

-- Properties of zero
theorem add_zero (a : ℤ) : a + 0 = a :=
by ring

theorem zero_add (a : ℤ) : 0 + a = a :=
by ring

theorem mul_zero (a : ℤ) : a * 0 = 0 :=
by ring

theorem zero_mul (a : ℤ) : 0 * a = 0 :=
by ring

-- Properties of negation
theorem add_left_neg (a : ℤ) : -a + a = 0 :=
by ring

theorem add_right_neg (a : ℤ) : a + -a = 0 :=
by ring

-- Distributivity
theorem dist_left (a b c : ℤ) : a * (b + c) = a * b + a * c :=
by ring

theorem dist_right (a b c : ℤ) : (a + b) * c = a * c + b * c :=
by ring

-- Other simple identities
theorem square_expansion (a b : ℤ) : (a + b) * (a + b) = a * a + 2 * a * b + b * b :=
by ring

theorem cube_expansion (a b : ℤ) : (a + b) * (a + b) * (a + b) = a^3 + 3 * a^2 * b + 3 * a * b^2 + b^3 :=
