| Within(Label)
| Other

@derive(Debug, Clone, Copy)
pub enum OneVsAll<Label, const label: Label>
| Yes
| No

impl<Label, const label: Label> core::ops::Unveil OneVsAll Label label for Class Label:
    type Output = ()

@derive(Debug, Clone, Copy)
pub enum OneVsAllResult<Label, const label: Label>
| ConfidentYes
| ConfidentNo
| Unconfident

impl<Label, const label: Label> core::ops::Unveil OneVsAllResult Label label for OneVsAll Label label:
    type Output = ()

