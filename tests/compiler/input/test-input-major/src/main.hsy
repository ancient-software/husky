use cv::datasets::mnist::BinaryImage28
use cv::datasets::mnist::MnistLabel

task:
    ml::datasets::cv::mnist::new_binary_dataset()

proc find_connected_components(binary_image: BinaryImage28) -> i32:
    let mut b = binary_image.clone()
    b[0] = 1r32
    return 1

main:
    if find_connected_components(input) == 1:
        MnistLabel::One
    else:
        MnistLabel::Two