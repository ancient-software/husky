func add(x: i32, y: i32) -> i32:
    x + y

proc bitor_assign() -> bool:
    let mut b = 0r32
    assert !0r32
    assert true && true
    assert !(true && false)
    assert 1 != 2
    let mut a = 1
    a--
    assert a == 0
    a++
    assert a == 1
    b |= 1r32
    return b == 1r32

proc bitand_assign() -> bool:
    let mut b = 0r32
    b &= 1r32
    return b == 0r32

task:
