proc f1() -> i32:
    let mut a = 0
    for 0 <= i < 5:
        a += i
        if i > 2:
            break
    return a

proc f2() -> i32:
    let mut a = 0
    for 0 <= i < 5:
        a += i
        if i > 2:
            break
        a += 1
    return a

proc f3() -> i32:
    let mut a = 0
    for 0 <= i < 5:
        a += i
        if i > 3:
            break
        if i > 2:
            break
        a += 1
    return a

task:
    ml::datasets::synthetic::trivial::real1d::dataset1()

main:
    assert f1() == 6
    assert f2() == 9
    assert f3() == 9
    1