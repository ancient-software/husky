task:
    ml::datasets::synthetic::trivial::real1d::dataset1()
    
struct A:
    mut x: i32

    // derived field
    func y -> i32:
        x + 1

    func get_x() -> i32:
        x

    func get_x_plus_constant() -> i32:
        c = 2
        x + c

    func get_x_squared() -> i32:
        x * x

proc f1() -> A:
    return A(1)

proc f2() -> A:
    let mut a = A(2)
    a.x = 1
    return a

proc f3():
    let a = A(2)
    assert a.get_x() == 2

func g1() -> i32:
    a = A(2)
    return a.x

main:
    a = f1()
    assert a.x == 1
    assert a.y == 2
    assert a.get_x() == 1
    assert a.get_x_plus_constant() == 3
    assert a.get_x_squared() == 1
    assert g1() == 2
    assert f2().x == 1
    1